@{Layout = string.Empty;}
<html>
<head>
    <title>test</title>


    <link href="http://www.internetke.com/jsEffects/2014101006/css/stylesheets/style.css" rel="stylesheet"/>
    <script src="https://pagead2.googlesyndication.com/pub-config/r20160212/ca-pub-2416180516640221.js"></script>
    <script src="http://www.internetke.com/jsEffects/2014101006/js/jquery-1.7.2.min.js"></script>
</head>
<body>

    <div class="kePublic">

        <!--效果html开始-->

        <div id="background"></div>
        <div id="player">
            <div class="cover"></div>
            <div class="ctrl">
                <div class="tag">
                    <strong>Title</strong>
                    <span class="artist">Artist</span>
                    <span class="album">Album</span>
                </div>
                <div class="control">
                    <div class="left">
                        <div class="rewind icon"></div>
                        <div class="playback icon"></div>
                        <div class="fastforward icon"></div>
                    </div>
                    <div class="volume right">
                        <div class="mute icon left"></div>
                        <div class="slider left">
                            <div class="pace"></div>
                        </div>
                    </div>
                </div>
                <div class="progress">
                    <div class="slider">
                        <div class="loaded"></div>
                        <div class="pace"></div>
                    </div>
                    <div class="timer left">0:00</div>
                    <div class="right">
                        <div class="repeat icon"></div>
                        <div class="shuffle icon"></div>
                    </div>
                </div>
            </div>
        </div>
        <ul id="playlist"></ul>

        <script src="http://www.internetke.com/jsEffects/2014101006/js/jquery-ui-1.8.17.custom.min.js"></script>
        <script src="/audio/audio.js"></script>

        <!--效果html结束-->

    </div>
    <script>
            
    </script>
</body>
</html>